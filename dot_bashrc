export LESS='-giMRSj10'
export PATH="/usr/include:/usr/local/include:$HOME/.local/bin:$PATH"
export DISPLAY="localhost:0.0"
alias ls='ls -a -I "NTUSER.DAT*" --color=auto'
alias ll='ls -l'
alias rm='rm -v'
alias showpath='echo $PATH | tr '\'':'\'' '\''\n'\'
alias relogin='exec $SHELL -l'
alias sshv='cocot -- ssh cd997'

